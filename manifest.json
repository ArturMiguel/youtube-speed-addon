{
    "manifest_version": 2,
    "name": "youtube-speed",
    "version": "1.0.2",
    "description": "Firefox extension to modify YouTube playback rate.",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "./assets/icon-48.png",
        "default_title": "Speed",
        "default_popup": "./index.html"
    },
    "icons": {
        "48": "./assets/icon-48.png",
        "96": "./assets/icon-96.png"
    }
}
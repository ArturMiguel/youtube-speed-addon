{
    "manifest_version": 2,
    "name": "youtube-speed-extension",
    "version": "1.0.1",
    "description": "Extensão para modificar a velocidade de reprodução de vídeos no Youtube",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "./assets/icon-48.png",
        "default_title": "Speed",
        "default_popup": "./index.html"
    },
    "icons": {
        "48": "./assets/icon-48.png",
        "96": "./assets/icon-96.png"
    }
}